{
	"name": "protoc_bundle",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"init" : "node scripts/init.js",
		"compile:descriptor" : "npx pbjs -t json -o ./generated/descriptor.json ./proto/*.proto",
		"compile:proto": "npx protoc --js_out=import_style=commonjs,binary:./generated -I ./proto proto/*.proto",
		"compile" : "npm run compile:descriptor && npm run compile:proto",
		"create:entry": "node scripts/create-entry.js",
		"bundle": "webpack",
		"build": "npm run init && npm run compile && npm run create:entry && npm run bundle"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"google-protobuf": "^3.21.4",
		"webpack": "^5.101.1",
		"webpack-cli": "^6.0.1"
	}
}
